	<parameter name="nroSolicitud" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT PROFESOR.*,
       VTD.TITULO,
       VTD.UNIVERSIDAD_OBTENCION,
       VTD.PAIS_OBTENCION,
       AREA.NOMBRE_AREA,
       PROGRAMA.NOMBRE_PROGRAMA, PROGRAMA.COORDINADOR,
       CONTRATO_DOCENTE.*,
 TITULO, UNIVERSIDAD_OBTENCION, PAIS_OBTENCION , REGISTRO_SENECYT,
 (SELECT PERSONAL.NOMBRE_COMPLETO  FROM interfaseOcu.GESTIONDOCENTE.TIPO_ESTADO TPE
  LEFT OUTER JOIN (select CODNUM,NOMBRE_COMPLETO  FROM
  OPENQUERY(OCU,'select CODNUM,NOMPRS || '' '' || LL1PRS || '' '' || LL2PRS NOMBRE_COMPLETO,DNIPRS  from VUIB_DATOSPERSONALES DP  WHERE DP.FLGPAS = ''S''') ) AS PERSONAL
  ON (TPE.STA_FIRMA_USR_SIG_PERFIL = PERSONAL.CODNUM) WHERE TPE.STA_CODIGO = 'S') FIRMA_REVISADO_RRHH,
 (SELECT PERSONAL.NOMBRE_COMPLETO  FROM interfaseOcu.GESTIONDOCENTE.TIPO_ESTADO TPE
  LEFT OUTER JOIN (select CODNUM,NOMBRE_COMPLETO  FROM
  OPENQUERY(OCU,'select CODNUM,NOMPRS || '' '' || LL1PRS || '' '' || LL2PRS NOMBRE_COMPLETO,DNIPRS  from VUIB_DATOSPERSONALES DP  WHERE DP.FLGPAS = ''S''') ) AS PERSONAL
  ON (TPE.STA_FIRMA_USR_SIG_PERFIL = PERSONAL.CODNUM) WHERE TPE.STA_CODIGO = 'H') FIRMA_REVISADO_FIN,
 (SELECT PERSONAL.NOMBRE_COMPLETO
  FROM interfaseOcu.GESTIONADMIN.AUTORIDAD AUTORIDAD
       INNER JOIN interfaseOcu.GESTIONADMIN.PARAMETROS PARAMETROS ON (AUTORIDAD.AUT_CODIGO = PARAMETROS.PRM_COD_RECTOR)
   LEFT OUTER JOIN (select CODNUM,NOMBRE_COMPLETO  FROM
  OPENQUERY(OCU,'select CODNUM,NOMPRS || '' '' || LL1PRS || '' '' || LL2PRS NOMBRE_COMPLETO,DNIPRS  from VUIB_DATOSPERSONALES DP  WHERE DP.FLGPAS = ''S''') ) AS PERSONAL
  ON (AUTORIDAD.AUT_COD_PERSONAL = PERSONAL.CODNUM)
   WHERE PARAMETROS.PRM_CODIGO = 1) FIRMA_AUTORIZADO_RECTORADO,
 (SELECT V_USUARIO.NOMBRES+' '+V_USUARIO.APELLIDOS FROM interfaseOcu.dbo.V_USUARIO where ID_PERFIL = 'P_COMUN' AND CODIGO = CONTRATO_DOCENTE.CDO_USU_CREA ) REGISTRADO,
       (SELECT  R.RDO_DESCRIPCION FROM  interfaseOcu.GESTIONDOCENTE.ROL_DOCENTE R WHERE convert(varchar(1),R.RDO_CODIGO) = convert(varchar(1),CONTRATO_DOCENTE.RDO_CODIGO))  RDO_DESCRIPCION,
       (SELECT FP.TFP_DESCRIPCION FROM  interfaseOcu.GESTIONDOCENTE.TIPOS_FORMA_PAGO FP WHERE FP.TFP_CODIGO = CONTRATO_DOCENTE.TFP_CODIGO) TFP_DESCRIPCION,
       (SELECT sum(HON_CANTIDAD*HON_VALOR) FROM  interfaseOcu.GESTIONDOCENTE.RUBROS RUBROS
		 INNER JOIN interfaseOcu.GESTIONDOCENTE.HONORARIOS HONORARIOS ON RUBROS.RUB_CODIGO = HONORARIOS.RUB_CODIGO
       WHERE RUBROS.RUB_CATEGORIA = 'V' and HONORARIOS.CDO_CODIGO = CONTRATO_DOCENTE.CDO_CODIGO) TOTAL_VIATICOS
  FROM (((interfaseOcu.GESTIONDOCENTE.CONTRATO_DOCENTE CONTRATO_DOCENTE
          INNER JOIN interfaseOcu.dbo.PROGRAMA PROGRAMA ON (CONTRATO_DOCENTE.ARE_CODIGO = PROGRAMA.ARE_CODIGO AND CONTRATO_DOCENTE.PRG_CODIGO = PROGRAMA.PRG_CODIGO AND CONTRATO_DOCENTE.CDO_ANIO_ACAD =PROGRAMA.ANIO))
         INNER JOIN interfaseOcu.dbo.PROFESOR PROFESOR ON (PROFESOR.CODIGO_PROFESOR = CONTRATO_DOCENTE.PRF_CODIGO))
        RIGHT OUTER JOIN interfaseOcu.GESTIONDOCENTE.V_TITULOS_DOCENTE VTD ON (VTD.PRS_CODNUM = PROFESOR.CODIGO_PROFESOR))
       INNER JOIN interfaseOcu.dbo.AREA AREA ON (AREA.ARE_CODIGO = PROGRAMA.ARE_CODIGO)
 WHERE CONTRATO_DOCENTE.CDO_CODIGO = $P{nroSolicitud}]]>
	</queryString>
