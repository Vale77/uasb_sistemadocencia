	<subDataset name="promedioHistorico" uuid="67ae84ec-e14a-4730-a6d9-f96a70e24058">
		<parameter name="codigoEsp" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT  ANIO, CODIGO_esp, CONVERT(DECIMAL(8, 2),AVG( VALOR)) AS VALOR
FROM interfaseOcu.EVALUACION.VISTA_TABULACION_EVALDOCENTEANIO as VEVAL
WHERE VEVAL.codigo_esp =$P{codigoEsp} and
VEVAL.TIPO_ENCUESTA = 'P'
GROUP BY ANIO, codigo_esp
order by 1]]>
		</queryString>
		<field name="ANIO" class="java.math.BigDecimal"/>
		<field name="CODIGO_esp" class="java.math.BigDecimal"/>
		<field name="VALOR" class="java.math.BigDecimal"/>
	</subDataset>
	<subDataset name="Comparativo" uuid="410394a0-c0cb-435d-8538-807c544447c7">
		<parameter name="codigoEsp" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="anio" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="codigoFacultad" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT 'Promedio del Programa' as TIPO, CONVERT(DECIMAL(8, 2), AVG( VALOR)) AS VALOR
FROM interfaseOcu.EVALUACION.VISTA_TABULACION_EVALDOCENTEANIO AS VEVAL
WHERE VEVAL.codigo_esp =$P{codigoEsp}  and
VEVAL.anio= $P{anio} and
VEVAL.TIPO_ENCUESTA = 'P'
union all
SELECT 'Promedio del Área' as TIPO,  CONVERT(DECIMAL(8, 2),AVG( VALOR)) AS VALOR
FROM interfaseOcu.EVALUACION.VISTA_TABULACION_EVALDOCENTEANIO AS VEVAL INNER JOIN interfaseOcu.dbo.PROGRAMA PROG
ON VEVAL.ANIO = PROG.ANIO
AND VEVAL.CODIGO_ESP = PROG.PRG_CODIGO
WHERE PROG.ARE_CODIGO = $P{codigoFacultad}
and VEVAL.anio= $P{anio}
and VEVAL.TIPO_ENCUESTA = 'P']]>
		</queryString>
		<field name="TIPO" class="java.lang.String"/>
		<field name="VALOR" class="java.math.BigDecimal"/>
	</subDataset>
	<parameter name="anio" class="java.lang.String"/>
	<parameter name="tipReporte" class="java.lang.String"/>
	<parameter name="codArea" class="java.lang.String"/>
	<parameter name="codEsp" class="java.lang.String"/>
	<queryString>
		<![CDATA[DECLARE @MATERIA TABLE ( AREA VARCHAR(75), PROGRAMA VARCHAR(500),CODIGO_ESP VARCHAR(4), CODIGO_FACULTAD NUMERIC(7,0),  ANIO INT, CODIGO_NIVEACAD VARCHAR(3), CICLO VARCHAR(384))
DECLARE @MATEST TABLE ( ANIO INT,CODIGO_ESP VARCHAR(4), NUM_EST INT)
INSERT INTO @MATERIA(AREA, PROGRAMA ,CODIGO_ESP , CODIGO_FACULTAD ,  ANIO , CODIGO_NIVEACAD , CICLO )
SELECT DISTINCT  ARE_NOMBRE, NOMBRE_PROGRAMA ,PRG_CODIGO , ARE_CODIGO ,  ANIO , COD_NIVEL_ACAD , 1
FROM interfaseOcu.dbo.PROGRAMA WITH (NOLOCK)
where anio = $P{anio}
INSERT INTO @MATEST  ( ANIO ,CODIGO_ESP, NUM_EST )
SELECT DISTINCT ANIO, CODIGO_ESP, NUMEST
FROM interfaseOcu.dbo.VISTA_NUMESTUDIANTE_PROGRAMA WITH (NOLOCK)
WHERE ANIO = $P{anio}
SELECT DISTINCT CICLO_ACADEMICO.NOMBRE_CICLO,CODIGO_FACULTAD,area as NOMBRE_FACULTAD,PROGRAMA AS programa,
VEVAL.ANIO,VEVAL.CODIGO_ENCUESTA, VEVAL.CODIGO_PREGUNTA, VEVAL.PREGUNTA,
CONVERT(DECIMAL(8, 3),VEVAL.VALOR) AS VALOR, VEVAL.NE2010,
interfaseOcu.EVALUACION.f_agrupa_observacion_evaluacion(VEVAL.ANIO, 1,VEVAL.CODIGO_ESP,-1,VEVAL.CODIGO_ENCUESTA, -1) AS obs,
NUM_EST AS NUM_ALUMNOS,
VEVAL.CODIGO_ESP, CODIGO_NIVEACAD
FROM  interfaseOcu.EVALUACION.VISTA_TABULACION_EVALDOCENTEANIO AS VEVAL WITH (NOLOCK) INNER JOIN interfaseOcu.dbo.CICLO_ACADEMICO WITH (NOLOCK)
ON VEVAL.ANIO  = CICLO_ACADEMICO.ANIO iNNER JOIN @MATERIA AS MATERIA
ON VEVAL.ANIO  = MATERIA.ANIO
AND VEVAL.CODIGO_ESP = MATERIA.CODIGO_ESP INNER JOIN @MATEST AS MATEST
ON VEVAL.ANIO  = MATEST.ANIO
AND VEVAL.CODIGO_ESP = MATEST.CODIGO_ESP
WHERE VEVAL.ANIO =$P{anio}
AND VEVAL.TIPO_ENCUESTA ='P'
and VEVAL.CODIGO_MATERIA = -1
and VEVAL.CODIGO_PROFESOR = -1
and VEVAL.CODIGO_NIVEL = -1
AND (CASE when(( $P{tipReporte}='A' OR $P{tipReporte} ='P' )and $P{codArea} <>'T')  then  MATERIA.CODIGO_FACULTAD ELSE 0 END )= (CASE  when(($P{tipReporte} ='A' OR $P{tipReporte} ='P' )and $P{codArea} <>'T')THEN $P{codArea} ELSE 0 END)
AND (CASE when($P{tipReporte}='P' and $P{codEsp} <> 'T') THEN VEVAL.CODIGO_ESP ELSE 0 END )= (CASE when($P{tipReporte} ='P'and $P{codEsp} <> 'T')THEN $P{codEsp} ELSE 0 END  )]]>
	</queryString>
