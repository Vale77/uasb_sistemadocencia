	<subDataset name="promedio_historico" uuid="406b12a1-e71e-4083-9da4-ab39d453747d">
		<parameter name="codEsp" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="numTuto" class="java.math.BigDecimal"/>
		<queryString>
			<![CDATA[SELECT  VEVAL.ANIO_EVAL ANIO,  AVG(CONVERT(DECIMAL(8, 2), VEVAL.VALOR)) AS promedio, SUM(DISTINCT VEVAL.NE2010) numTutorias
FROM interfaseOcu.EVALUACION.VISTA_TAB_EVALTESIS_ANIO AS VEVAL
WHERE VEVAL.TIPO_ENCUESTA = 'T'
AND VEVAL.CODIGO_ESP = $P{codEsp}
AND VEVAL.TIPO_PREGUNTA <>3
AND VEVAL.CODIGO_PREGUNTA NOT IN(11,12)
GROUP BY VEVAL.ANIO_EVAL
order by 1]]>
		</queryString>
		<field name="ANIO" class="java.math.BigDecimal"/>
		<field name="promedio" class="java.math.BigDecimal"/>
		<field name="numTutorias" class="java.lang.Integer"/>
	</subDataset>
	<subDataset name="promedioporpregunta" uuid="e6372dbb-ff78-49c1-8134-a9a27441bc57">
