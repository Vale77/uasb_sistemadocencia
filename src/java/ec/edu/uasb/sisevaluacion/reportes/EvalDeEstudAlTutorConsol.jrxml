	<queryString>
		<![CDATA[DECLARE @MATERIA TABLE ( AREA VARCHAR(75), PROGRAMA VARCHAR(500),CODIGO_ESP VARCHAR(4), CODIGO_FACULTAD NUMERIC(7,0),  ANIO INT, CODIGO_NIVEACAD VARCHAR(3))

INSERT INTO @MATERIA(AREA, PROGRAMA ,CODIGO_ESP , CODIGO_FACULTAD ,  ANIO , CODIGO_NIVEACAD  )
SELECT distinct  ARE_NOMBRE, NOMBRE_PROGRAMA ,PRG_CODIGO , ARE_CODIGO ,  ANIO , COD_NIVEL_ACAD
FROM interfaseOcu.dbo.PROGRAMA

select DISTINCT CICLO_ACADEMICO.NOMBRE_CICLO,VEVAL.ANIO_EVAL ANIO,VEVAL.ANIO ANIO_MAT, VEVAL.CODIGO_ENCUESTA,
MATERIA.PROGRAMA AS PROGRAMA,
CONVERT(DECIMAL(8, 3),AVG(CASE WHEN VEVAL.CODIGO_PREGUNTA=21 THEN VEVAL.VALOR END)) AS PROM_PREG1,
CONVERT(DECIMAL(8, 3),AVG(CASE WHEN VEVAL.CODIGO_PREGUNTA=22 THEN VEVAL.VALOR END)) AS PROM_PREG2,
CONVERT(DECIMAL(8, 3),AVG(CASE WHEN VEVAL.CODIGO_PREGUNTA=23 THEN VEVAL.VALOR END)) AS PROM_PREG3,
CONVERT(DECIMAL(8, 3),AVG(CASE WHEN VEVAL.CODIGO_PREGUNTA=24 THEN VEVAL.VALOR END)) AS PROM_PREG4,
CONVERT(DECIMAL(8, 3),AVG(CASE WHEN VEVAL.CODIGO_PREGUNTA=25 THEN VEVAL.VALOR END)) AS PROM_PREG5,
CONVERT(DECIMAL(8, 3),AVG(CASE WHEN VEVAL.CODIGO_PREGUNTA=26 THEN VEVAL.VALOR END)) AS PROM_PREG6,
CONVERT(DECIMAL(8, 3),AVG(CASE WHEN VEVAL.CODIGO_PREGUNTA=27 THEN VEVAL.VALOR END)) AS PROM_PREG7,
CONVERT(DECIMAL(8, 3),AVG(CASE WHEN VEVAL.CODIGO_PREGUNTA=28 THEN VEVAL.VALOR END)) AS PROM_PREG8,
CONVERT(DECIMAL(8, 3),AVG(CASE WHEN VEVAL.CODIGO_PREGUNTA=29 THEN VEVAL.VALOR END)) AS PROM_PREG9,
MAX(VEVAL.NE2010) AS NE2010,
MATERIA.AREA AS NOMBRE_FACULTAD, MATERIA.codigo_niveacad,
VEVAL.CODIGO_PROFESOR, PROFESOR.APELLIDO_PROFESOR, PROFESOR.NOMBRE_PROFESOR, VEVAL.CODIGO_ESP,
MATERIA.CODIGO_FACULTAD
from interfaseOcu.EVALUACION.VISTA_TAB_EVALTESIS_ANIO as VEVAL  INNER JOIN interfaseOcu.dbo.CICLO_ACADEMICO
ON VEVAL.ANIO  = CICLO_ACADEMICO.ANIO iNNER JOIN @MATERIA AS MATERIA
ON VEVAL.ANIO  = MATERIA.ANIO
AND VEVAL.CODIGO_ESP = MATERIA.CODIGO_ESP INNER JOIN interfaseOcu.dbo.PROFESOR
ON PROFESOR.CODIGO_PROFESOR = VEVAL.CODIGO_PROFESOR
WHERE VEVAL.TIPO_ENCUESTA = 'T'
AND VEVAL.ANIO_EVAL =$P{anio}
AND (CASE when(($P{tipReporte} ='A' OR $P{tipReporte} ='P')and $P{codArea} <>'T')  then  MATERIA.CODIGO_FACULTAD ELSE '0' END )= (CASE  when(($P{tipReporte} ='A' OR $P{tipReporte} ='P')and $P{codArea} <>'T')THEN $P{codArea}  ELSE '0' END)
AND (CASE when(($P{tipReporte}='P')and $P{codEsp} <> 'T') THEN VEVAL.CODIGO_ESP ELSE '0' END )= (CASE when(($P{tipReporte} ='P')and $P{codEsp} <> 'T')THEN  $P{codEsp} ELSE '0' END  )
GROUP BY CICLO_ACADEMICO.NOMBRE_CICLO,MATERIA.PROGRAMA,VEVAL.ANIO_EVAL,VEVAL.ANIO, VEVAL.CODIGO_ENCUESTA, MATERIA.AREA,MATERIA.codigo_niveacad, VEVAL.CODIGO_PROFESOR, PROFESOR.APELLIDO_PROFESOR, PROFESOR.NOMBRE_PROFESOR, VEVAL.CODIGO_ESP, MATERIA.CODIGO_FACULTAD]]>
	</queryString>
