	<queryString>
		<![CDATA[DECLARE @MATERIA TABLE ( AREA VARCHAR(75), PROGRAMA VARCHAR(500),CODIGO_ESP VARCHAR(4), CODIGO_FACULTAD NUMERIC(7,0),  ANIO INT, CODIGO_NIVEACAD VARCHAR(3), CICLO VARCHAR(384))
INSERT INTO @MATERIA(AREA, PROGRAMA ,CODIGO_ESP , CODIGO_FACULTAD ,  ANIO , CODIGO_NIVEACAD , CICLO)
SELECT distinct  ARE_NOMBRE, NOMBRE_PROGRAMA ,PRG_CODIGO , ARE_CODIGO,  ANIO , COD_NIVEL_ACAD , 1
FROM interfaseOcu.dbo.PROGRAMA with (NOLOCK)
where anio = $P{anio}
SELECT  DISTINCT CICLO_ACADEMICO.NOMBRE_CICLO, AREA, PROGRAMA,
ENCCAL.ANIO, ENCCAL.CICLO,ENCCAL.CODIGO_ENCUESTA,
ENCCAL.CODIGO_ESP
FROM @MATERIA MATERIA  INNER JOIN interfaseOcu.EVALUACION.ENCUESTA_CALENDARIO ENCCAL with (NOLOCK)
ON MATERIA.ANIO = ENCCAL.ANIO
AND MATERIA.CICLO = ENCCAL.CICLO
AND MATERIA.CODIGO_ESP = ENCCAL.CODIGO_ESP  INNER JOIN interfaseOcu.dbo.CICLO_ACADEMICO with (NOLOCK)
ON CICLO_ACADEMICO.ANIO = ENCCAL.ANIO
AND CICLO_ACADEMICO.CICLO = ENCCAL.CICLO
WHERE  ENCCAL.TIPO_ENCUESTA = 'O'
AND ENCCAL.CODIGO_MATERIA = -1
AND ENCCAL.CODIGO_PROFESOR = -1
AND ENCCAL.CODIGO_NIVEL = -1
AND ENCCAL.ANIO = $P{anio}
AND (CASE when(($P{tipReporte} ='A' OR $P{tipReporte} ='P')and $P{codArea} <>'T')  then  MATERIA.CODIGO_FACULTAD ELSE '0' END )= (CASE  when(($P{tipReporte} ='A' OR $P{tipReporte} ='P' )and $P{codArea} <>'T')THEN $P{codArea}  ELSE '0' END)
AND (CASE when(($P{tipReporte}='P')and $P{codEsp} <> 'T') THEN ENCCAL.CODIGO_ESP ELSE '0' END )= (CASE when(($P{tipReporte} ='P')and $P{codEsp} <> 'T')THEN  $P{codEsp} ELSE '0' END  )]]>
	</queryString>
