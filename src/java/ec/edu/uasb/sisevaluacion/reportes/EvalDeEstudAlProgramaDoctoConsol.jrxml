	<subDataset name="promedioporpregunta" uuid="f7f7aed8-91d8-4f9a-b5c5-6232556abd19">
		<parameter name="anio" class="java.lang.String"/>
		<parameter name="codArea" class="java.lang.String"/>
		<queryString>
			<![CDATA[DECLARE @MATERIA TABLE (CODIGO_ESP VARCHAR(4),  ANIO INT, CODIGO_NIVEACAD VARCHAR(3), CODIGO_FACULTAD NUMERIC(8,0))
INSERT INTO @MATERIA(CODIGO_ESP ,  ANIO , CODIGO_NIVEACAD , CODIGO_FACULTAD )
SELECT distinct   PRG_CODIGO , ANIO , COD_NIVEL_ACAD , ARE_CODIGO
FROM interfaseOcu.dbo.PROGRAMA
where anio = $P{anio}
SELECT DISTINCT  VEVAL.CODIGO_PREGUNTA,VEVAL.PREGUNTA, CONVERT(DECIMAL(8, 2),AVG( VEVAL.VALOR)) AS VALOR
FROM interfaseOcu.EVALUACION.VISTA_TABULACION_EVALDOCENTEANIO as VEVAL INNER JOIN @MATERIA AS MATERIA
ON VEVAL.ANIO  = MATERIA.ANIO
AND VEVAL.CODIGO_ESP = MATERIA.CODIGO_ESP
WHERE VEVAL.TIPO_ENCUESTA = 'P'
AND MATERIA.CODIGO_NIVEACAD = 3
AND VEVAL.CODIGO_ENCUESTA =21
AND MATERIA.CODIGO_FACULTAD = $P{codArea}
 AND VEVAL.ANIO = $P{anio}
GROUP BY VEVAL.CODIGO_PREGUNTA,VEVAL.PREGUNTA]]>
		</queryString>
		<field name="CODIGO_PREGUNTA" class="java.math.BigDecimal"/>
		<field name="PREGUNTA" class="java.lang.String"/>
		<field name="VALOR" class="java.math.BigDecimal"/>
		<sortField name="CODIGO_PREGUNTA"/>
	</subDataset>
	<subDataset name="promedioHistorico" uuid="f379e1f5-cdc0-4052-a112-be8643aa1c27">
		<parameter name="codArea" class="java.lang.String"/>
		<queryString>
			<![CDATA[DECLARE @MATERIA TABLE ( CODIGO_ESP VARCHAR(4), ANIO int, CODIGO_NIVEACAD VARCHAR(3), CODIGO_FACULTAD NUMERIC(8,0))
INSERT INTO @MATERIA(CODIGO_ESP ,ANIO,  CODIGO_NIVEACAD , CODIGO_FACULTAD )
SELECT distinct  PRG_CODIGO , ANIO , COD_NIVEL_ACAD , ARE_CODIGO
FROM interfaseOcu.dbo.PROGRAMA
SELECT DISTINCT VEVAL.ANIO,CONVERT(DECIMAL(8, 2),AVG(VEVAL.VALOR)) AS VALOR
FROM interfaseOcu.EVALUACION.VISTA_TABULACION_EVALDOCENTEANIO AS VEVAL iNNER JOIN @MATERIA AS MATERIA
ON VEVAL.ANIO  = MATERIA.ANIO
AND VEVAL.CODIGO_ESP = MATERIA.CODIGO_ESP
WHERE VEVAL.TIPO_ENCUESTA = 'P'
AND MATERIA.CODIGO_NIVEACAD = 3
AND VEVAL.CODIGO_ENCUESTA =21
AND MATERIA.CODIGO_FACULTAD = $P{codArea}
GROUP BY VEVAL.anio]]>
		</queryString>
		<field name="ANIO" class="java.math.BigDecimal"/>
		<field name="VALOR" class="java.math.BigDecimal"/>
	</subDataset>
	<parameter name="tipReporte" class="java.lang.String"/>
	<parameter name="anio" class="java.lang.String"/>
	<parameter name="codArea" class="java.lang.String"/>
	<parameter name="codEsp" class="java.lang.String"/>
	<queryString>
		<![CDATA[DECLARE @MATERIA TABLE ( AREA VARCHAR(75), PROGRAMA VARCHAR(500),CODIGO_ESP VARCHAR(4), CODIGO_FACULTAD NUMERIC(7,0),  ANIO INT, CODIGO_NIVEACAD VARCHAR(3))
DECLARE @MATEST TABLE ( ANIO INT,CODIGO_ESP VARCHAR(4), NUM_EST INT)
INSERT INTO @MATERIA(AREA, PROGRAMA ,CODIGO_ESP , CODIGO_FACULTAD ,  ANIO , CODIGO_NIVEACAD )
SELECT distinct  ARE_NOMBRE, NOMBRE_PROGRAMA ,PRG_CODIGO , ARE_CODIGO ,  ANIO , COD_NIVEL_ACAD
FROM interfaseOcu.dbo.PROGRAMA
where anio = $P{anio}
INSERT INTO @MATEST  ( ANIO ,CODIGO_ESP, NUM_EST )
SELECT DISTINCT ANIO, CODIGO_ESP, NUMEST
FROM interfaseOcu.dbo.VISTA_NUMESTUDIANTE_PROGRAMA
WHERE ANIO = $P{anio}
select DISTINCT  CICLO_ACADEMICO.NOMBRE_CICLO,
MATERIA.PROGRAMA AS programa,
VEVAL.ANIO, VEVAL.CODIGO_ENCUESTA,
CONVERT(DECIMAL(8, 3),AVG(CASE WHEN VEVAL.CODIGO_PREGUNTA=1 THEN VEVAL.VALOR END)) AS PROM_PREG1,
CONVERT(DECIMAL(8, 3),AVG(CASE WHEN VEVAL.CODIGO_PREGUNTA=2 THEN VEVAL.VALOR END)) AS PROM_PREG2,
CONVERT(DECIMAL(8, 3),AVG(CASE WHEN VEVAL.CODIGO_PREGUNTA=3 THEN VEVAL.VALOR END)) AS PROM_PREG3,
CONVERT(DECIMAL(8, 3),AVG(CASE WHEN VEVAL.CODIGO_PREGUNTA=4 THEN VEVAL.VALOR END)) AS PROM_PREG4,
CONVERT(DECIMAL(8, 3),AVG(CASE WHEN VEVAL.CODIGO_PREGUNTA=5 THEN VEVAL.VALOR END)) AS PROM_PREG5,
MAX(VEVAL.NE2010) AS NE_R,
MAX(MATEST.NUM_EST ) AS NE_P,
MATERIA.AREA AS area, MATERIA.codigo_niveacad,
VEVAL.CODIGO_ESP, MATERIA.CODIGO_FACULTAD
from interfaseOcu.EVALUACION.VISTA_TABULACION_EVALDOCENTEANIO as VEVAL  INNER JOIN interfaseOcu.dbo.CICLO_ACADEMICO WITH (NOLOCK)
ON VEVAL.ANIO  = CICLO_ACADEMICO.ANIO iNNER JOIN @MATERIA AS MATERIA
ON VEVAL.ANIO  = MATERIA.ANIO
AND VEVAL.CODIGO_ESP = MATERIA.CODIGO_ESP INNER JOIN @MATEST AS MATEST
 ON VEVAL.ANIO  = MATEST.ANIO
 AND VEVAL.CODIGO_ESP = MATEST.CODIGO_ESP
WHERE VEVAL.TIPO_ENCUESTA = 'P'
AND VEVAL.codigo_encuesta = 21
AND MATERIA.CODIGO_NIVEACAD =3
AND VEVAL.ANIO =$P{anio}
and VEVAL.CODIGO_MATERIA = -1
and VEVAL.CODIGO_PROFESOR = -1
and VEVAL.CODIGO_NIVEL = -1
AND (CASE when(($P{tipReporte} ='A' OR $P{tipReporte} ='P')and $P{codArea} <>'T')  then  MATERIA.CODIGO_FACULTAD ELSE '0' END )= (CASE  when(($P{tipReporte} ='A' OR $P{tipReporte} ='P')and $P{codArea} <>'T')THEN $P{codArea}  ELSE '0' END)
AND (CASE when(($P{tipReporte}='P')and $P{codEsp} <> 'T') THEN VEVAL.CODIGO_ESP ELSE '0' END )= (CASE when(($P{tipReporte} ='P' )and $P{codEsp} <> 'T')THEN  $P{codEsp} ELSE '0' END  )
GROUP BY CICLO_ACADEMICO.NOMBRE_CICLO,MATERIA.PROGRAMA,VEVAL.ANIO, VEVAL.CODIGO_ENCUESTA, MATERIA.AREA,MATERIA.codigo_niveacad, VEVAL.CODIGO_ESP,MATERIA.CODIGO_FACULTAD]]>
	</queryString>
