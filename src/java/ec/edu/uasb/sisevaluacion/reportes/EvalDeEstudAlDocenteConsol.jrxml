	<subDataset name="promHistPrograma" uuid="242f91cf-ade5-4f8b-aa70-17fe940cefac">
		<parameter name="codigoEsp" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select TABDOC.ANIO,convert(decimal(8,3),avg(ISNULL(TABDOC.VALOR,0))) as VALOR
 FROM interfaseOcu.EVALUACION.VISTA_TABULACION_EVALDOCENTEANIO AS TABDOC
 where TABDOC.TIPO_ENCUESTA = 'A'
 and TABDOC.CODIGO_ESP =$P{codigoEsp}
and TABDOC.CODIGO_PREGUNTA >(case when TABDOC.CODIGO_ENCUESTA = 17 then  4 else 0 end)
 GROUP by TABDOC.ANIO, TABDOC.CODIGO_ESP]]>
		</queryString>
		<field name="ANIO" class="java.math.BigDecimal"/>
		<field name="VALOR" class="java.math.BigDecimal"/>
		<sortField name="ANIO"/>
	</subDataset>
