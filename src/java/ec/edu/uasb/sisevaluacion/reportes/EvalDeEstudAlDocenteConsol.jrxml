	<queryString>
		<![CDATA[DECLARE @MATEST TABLE  (ANIO NUMERIC(4,0), CODIGO_ESP NUMERIC(10,0), CODIGO_MATERIA NUMERIC(10,0), CODIGO_PROFESOR NUMERIC(10,0),CODIGO_NIVEL NUMERIC(10,0), NUMEST NUMERIC(10,0), CODIGO_PARALELO INT)
DECLARE @MATERIA TABLE (CODIGO_PROFESOR NUMERIC(8,0), NOMBRE_PROFESOR VARCHAR(60), APELLIDO_PROFESOR VARCHAR(65), NOMBRE_MATERIA VARCHAR(200), AREA VARCHAR(75), CODIGO_NIVEL INT, PROGRAMA VARCHAR(500),NIVEL_ACADEMICO VARCHAR(75),
                         CODIGO_ESP VARCHAR(4),  CODIGO_MATERIA NUMERIC(9,0), ANIO INT,  TRIMESTRE VARCHAR(75),CICLO VARCHAR(384), NCREDITOS NUMERIC(5,2), CODIGO_FACULTAD NUMERIC(8,0), CODIGO_NIVEACAD NUMERIC(8,0), CODIGO_PARALELO INT, NOMBRE_PARALELO VARCHAR(10))
INSERT INTO @MATEST(ANIO, CODIGO_ESP , CODIGO_MATERIA, CODIGO_PROFESOR,CODIGO_NIVEL, NUMEST, CODIGO_PARALELO)
SELECT DISTINCT ANIO, CODIGO_ESP , CODIGO_MATERIA, CODIGO_PROFESOR,CODIGO_NIVEL, NUMEST, CODIGO_PARALELO
FROM interfaseOcu.dbo.VISTA_NUMESTUDIANTE_MATERIA WITH (NOLOCK)
WHERE ANIO =$P{anio}
INSERT INTO @MATERIA(CODIGO_PROFESOR,  NOMBRE_PROFESOR, APELLIDO_PROFESOR, NOMBRE_MATERIA, AREA, CODIGO_NIVEL, PROGRAMA,NIVEL_ACADEMICO,
CODIGO_ESP, CODIGO_MATERIA, ANIO,  TRIMESTRE,CICLO, NCREDITOS, CODIGO_FACULTAD, CODIGO_NIVEACAD, CODIGO_PARALELO, NOMBRE_PARALELO)
SELECT DISTINCT CODIGO_PROFESOR,  NOMBRE_PROFESOR, APELLIDO_PROFESOR, NOMBRE_MATERIA, AREA, CODIGO_NIVEL, PROGRAMA,NIVEL_ACADEMICO,
CODIGO_ESP, CODIGO_MATERIA, ANIO,  TRIMESTRE,CICLO, CREDITO_DOCENTE, CODIGO_FACULTAD, CODIGO_NIVEACAD,  COD_PARALELO, NOMBRE_PARALELO
FROM interfaseOcu.dbo.VISTA_COORDINADOR_PROGRAMA WITH (NOLOCK)
WHERE ANIO =$P{anio}

SELECT  DISTINCT CICLO_ACADEMICO.NOMBRE_CICLO , MATERIA.ANIO,MATERIA.AREA as NOMBRE_FACULTAD,
MATERIA.PROGRAMA AS programa,
MATERIA.CODIGO_NIVEL, MATERIA.TRIMESTRE,
MATERIA.APELLIDO_PROFESOR + ' ' + MATERIA.NOMBRE_PROFESOR AS nombre_profesor,
MATERIA.NOMBRE_MATERIA,
CONVERT(DECIMAL(8, 3),AVG(CASE WHEN PREG.tipo <> 3 THEN VEVAL.VALOR ELSE (CASE WHEN VEVAL.VALOR = 1.000000 THEN 5.000000 ELSE 1.000000 END) END)) AS VALOR,
MAX(VEVAL.NE2010) AS NE2010,
MAX(MATERIA.NCREDITOS* 10) AS horas_materia_dictar,
MATERIA.CODIGO_MATERIA,
CONVERT(DECIMAL(8, 2),AVG(CASE WHEN VEVAL.CODIGO_PREGUNTA=6 THEN ISNULL(VEVAL.VALOR,0) END)) AS P1,
CONVERT(DECIMAL(8, 2),AVG(CASE WHEN VEVAL.CODIGO_PREGUNTA=7 THEN ISNULL(VEVAL.VALOR,0) END)) AS P2,
CONVERT(DECIMAL(8, 2),AVG(CASE WHEN VEVAL.CODIGO_PREGUNTA=8 THEN ISNULL(VEVAL.VALOR,0) END)) AS P3,
CONVERT(DECIMAL(8, 2),AVG(CASE WHEN VEVAL.CODIGO_PREGUNTA=9 THEN ISNULL(VEVAL.VALOR,0)END)) AS P4,
CONVERT(DECIMAL(8, 2),AVG(CASE WHEN VEVAL.CODIGO_PREGUNTA=10 THEN ISNULL(VEVAL.VALOR,0) END)) AS P5,
CONVERT(DECIMAL(8, 2),AVG(CASE WHEN VEVAL.CODIGO_PREGUNTA=11 THEN ISNULL(VEVAL.VALOR,0) END)) AS P6,
VEVAL.CODIGO_PROFESOR,
MAX(MATEST.NUMEST) as NUM_ALUMNOS,
VEVAL.CODIGO_ESP,
MATERIA.CODIGO_NIVEACAD,
MATERIA.NOMBRE_PARALELO
FROM  interfaseOcu.EVALUACION.VISTA_TABULACION_EVALDOCENTEANIO AS VEVAL INNER JOIN @MATERIA AS MATERIA
ON VEVAL.ANIO  = MATERIA.ANIO
AND VEVAL.CODIGO_ESP = MATERIA.CODIGO_ESP
AND VEVAL.CODIGO_MATERIA = MATERIA.CODIGO_MATERIA
AND VEVAL.CODIGO_NIVEL = MATERIA.CODIGO_NIVEL
AND VEVAL.CODIGO_PROFESOR = MATERIA.CODIGO_PROFESOR
AND VEVAL.CODIGO_PARALELO = MATERIA.CODIGO_PARALELO INNER JOIN interfaseOcu.EVALUACION.PREGUNTA AS PREG WITH (NOLOCK)
ON VEVAL.CODIGO_ENCUESTA = PREG.CODIGO_ENCUESTA
AND VEVAL.CODIGO_PREGUNTA = PREG.CODIGO_PREGUNTA INNER JOIN @MATEST AS MATEST
ON VEVAL.ANIO  = MATEST.ANIO
AND VEVAL.CODIGO_ESP = MATEST.CODIGO_ESP
AND VEVAL.CODIGO_MATERIA = MATEST.CODIGO_MATERIA
AND VEVAL.CODIGO_NIVEL = MATEST.CODIGO_NIVEL
AND VEVAL.CODIGO_PROFESOR = MATEST.CODIGO_PROFESOR
AND VEVAL.CODIGO_PARALELO = MATEST.CODIGO_PARALELO inner join interfaseOcu.dbo.CICLO_ACADEMICO WITH (NOLOCK)
on VEVAL.ANIO = CICLO_ACADEMICO.ANIO
WHERE VEVAL.TIPO_ENCUESTA = 'A'
AND VEVAL.ANIO = $P{anio}
AND VEVAL.CODIGO_PREGUNTA > (CASE WHEN VEVAL.CODIGO_ENCUESTA = 17 THEN 4 ELSE 0 END)
AND (CASE when(($P{tipReporte} ='A' OR $P{tipReporte} ='P' OR $P{tipReporte} ='T')and $P{codArea} <>'T')  then  MATERIA.CODIGO_FACULTAD ELSE '0' END )= (CASE  when(($P{tipReporte} ='A' OR $P{tipReporte} ='P' OR $P{tipReporte} ='T')and $P{codArea} <>'T')THEN $P{codArea}  ELSE '0' END)
AND (CASE when(($P{tipReporte}='P' OR $P{tipReporte} ='T')and $P{codEsp} <> 'T') THEN VEVAL.CODIGO_ESP ELSE '0' END )= (CASE when(($P{tipReporte} ='P'OR $P{tipReporte} ='T')and $P{codEsp} <> 'T')THEN  $P{codEsp} ELSE '0' END  )
AND (CASE when($P{tipReporte} ='T'and $P{codTrim}<> 'T') THEN VEVAL.CODIGO_NIVEL ELSE '0' END )= (CASE when($P{tipReporte} ='T'and $P{codTrim} <> 'T')THEN  $P{codTrim}ELSE '0' END  )
GROUP BY CICLO_ACADEMICO.NOMBRE_CICLO,MATERIA.ANIO,MATERIA.AREA, MATERIA.NIVEL_ACADEMICO,
MATERIA.PROGRAMA, MATERIA.CODIGO_NIVEL,
MATERIA.TRIMESTRE, MATERIA.APELLIDO_PROFESOR, MATERIA.NOMBRE_PROFESOR,
MATERIA.NOMBRE_MATERIA, MATERIA.CODIGO_MATERIA,VEVAL.CODIGO_MATERIA, VEVAL.CODIGO_PROFESOR, VEVAL.CODIGO_ESP, MATERIA.CODIGO_NIVEACAD, MATERIA.NOMBRE_PARALELO]]>
	</queryString>
