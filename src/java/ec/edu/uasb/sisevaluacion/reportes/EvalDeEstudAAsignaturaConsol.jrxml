	<queryString>
		<![CDATA[DECLARE @MATEST TABLE  (ANIO NUMERIC(4,0), CODIGO_ESP NUMERIC(10,0), CODIGO_MATERIA NUMERIC(10,0), CODIGO_PROFESOR NUMERIC(10,0),CODIGO_NIVEL NUMERIC(10,0), NUMEST NUMERIC(10,0))
DECLARE @MATERIA TABLE (CODIGO_PROFESOR NUMERIC(8,0),  NOMBRE_PROFESOR VARCHAR(60), APELLIDO_PROFESOR VARCHAR(65), NOMBRE_MATERIA VARCHAR(200), AREA VARCHAR(75), CODIGO_NIVEL INT, PROGRAMA VARCHAR(500),
                 CODIGO_ESP VARCHAR(4),  CODIGO_MATERIA NUMERIC(9,0), ANIO INT,   TRIMESTRE VARCHAR(75), CICLO VARCHAR(384), NCREDITOS NUMERIC(5,2), CODIGO_FACULTAD NUMERIC(8,0), CODIGO_NIVEACAD NUMERIC(8,0), NUMCLASES NUMERIC(2,0))
INSERT INTO @MATEST(ANIO, CODIGO_ESP , CODIGO_MATERIA, CODIGO_PROFESOR,CODIGO_NIVEL, NUMEST)
SELECT DISTINCT ANIO, CODIGO_ESP , CODIGO_MATERIA, CODIGO_PROFESOR,CODIGO_NIVEL, NUMEST
FROM interfaseOcu.dbo.VISTA_NUMESTUDIANTE_MATERIA WITH (NOLOCK)
where anio = $P{anio}
INSERT INTO @MATERIA(CODIGO_PROFESOR,  NOMBRE_PROFESOR, APELLIDO_PROFESOR, NOMBRE_MATERIA, AREA, CODIGO_NIVEL, PROGRAMA, CODIGO_ESP,  CODIGO_MATERIA, ANIO,
			TRIMESTRE, CICLO, NCREDITOS, CODIGO_FACULTAD , CODIGO_NIVEACAD, NUMCLASES)
SELECT DISTINCT CODIGO_PROFESOR,  NOMBRE_PROFESOR, APELLIDO_PROFESOR, NOMBRE_MATERIA, AREA, CODIGO_NIVEL, PROGRAMA, CODIGO_ESP,  CODIGO_MATERIA, ANIO,TRIMESTRE, CICLO, CREDITO_DOCENTE, CODIGO_FACULTAD , CODIGO_NIVEACAD, NUMCLASES
FROM interfaseOcu.dbo.VISTA_COORDINADOR_PROGRAMA WITH (NOLOCK)
where anio = $P{anio}
select DISTINCT  CICLO_ACADEMICO.NOMBRE_CICLO,
VEVAL.ANIO, VEVAL.CICLO,
VEVAL.CODIGO_MATERIA, VEVAL.CODIGO_ENCUESTA,
MATERIA.NOMBRE_PROFESOR,
MATERIA.APELLIDO_PROFESOR, MATERIA.CODIGO_PROFESOR, MATERIA.NOMBRE_MATERIA, MATERIA.TRIMESTRE AS NOMBRE_NIVEL,
MATERIA.CODIGO_ESP,
MATERIA.AREA AS NOMBRE_FACULTAD, MATERIA.PROGRAMA,
MAX(MATERIA.NCREDITOS*10) AS HORAS_MATERIA_DICTAR,
Convert(decimal(8,2),avg(VEVAL.VALOR)) as VALOR,
MAX(VEVAL.NE2010) AS  ENC_REALIZADAS,
MAX(MATEST.NUMEST) AS NUM_ESTUDIANTES,
CONVERT(DECIMAL(8, 3),AVG(CASE WHEN VEVAL.CODIGO_PREGUNTA=1 THEN VEVAL.VALOR END)) as UNO,
CONVERT(DECIMAL(8, 3),AVG(CASE WHEN VEVAL.CODIGO_PREGUNTA=2 THEN VEVAL.VALOR END)) as DOS,
CONVERT(DECIMAL(8, 3),AVG(CASE WHEN VEVAL.CODIGO_PREGUNTA=3 THEN VEVAL.VALOR END)) as TRES,
CONVERT(DECIMAL(8, 3),AVG(CASE WHEN VEVAL.CODIGO_PREGUNTA=4 THEN VEVAL.VALOR END)) as CUATRO,
MATERIA.CODIGO_NIVEL,
MATERIA.CODIGO_NIVEACAD
from interfaseOcu.EVALUACION.VISTA_TAB_EVALASIGNATURA_ANIO as VEVAL WITH (NOLOCK) INNER JOIN @MATERIA AS MATERIA
ON VEVAL.ANIO  = MATERIA.ANIO
AND VEVAL.CODIGO_ESP = MATERIA.CODIGO_ESP
AND VEVAL.CODIGO_MATERIA = MATERIA.CODIGO_MATERIA
AND VEVAL.CODIGO_NIVEL = MATERIA.CODIGO_NIVEL
AND VEVAL.CODIGO_PROFESOR = MATERIA.CODIGO_PROFESOR  INNER JOIN @MATEST AS MATEST
ON VEVAL.ANIO  = MATEST.ANIO
AND VEVAL.CODIGO_ESP = MATEST.CODIGO_ESP
AND VEVAL.CODIGO_MATERIA = MATEST.CODIGO_MATERIA
AND VEVAL.CODIGO_NIVEL = MATEST.CODIGO_NIVEL
AND  VEVAL.CODIGO_PROFESOR = MATEST.CODIGO_PROFESOR inner join interfaseOcu.dbo.CICLO_ACADEMICO WITH (NOLOCK)
on VEVAL.ANIO = CICLO_ACADEMICO.ANIO
WHERE   VEVAL.ANIO =$P{anio}
AND (CASE when(($P{tipReporte} ='A' OR $P{tipReporte} ='P' OR $P{tipReporte} ='T')and $P{codArea} <>'T')  then  MATERIA.CODIGO_FACULTAD ELSE '0' END )= (CASE  when(($P{tipReporte} ='A' OR $P{tipReporte} ='P' OR $P{tipReporte} ='T')and $P{codArea} <>'T')THEN $P{codArea}  ELSE '0' END)
AND (CASE when(($P{tipReporte}='P' OR $P{tipReporte} ='T')and $P{codEsp} <> 'T') THEN VEVAL.CODIGO_ESP ELSE '0' END )= (CASE when(($P{tipReporte} ='P' OR $P{tipReporte} ='T')and $P{codEsp} <> 'T')THEN  $P{codEsp} ELSE '0' END  )
AND (CASE when($P{tipReporte} ='T' and $P{codTrim}<> 'T') THEN VEVAL.CODIGO_NIVEL ELSE '0' END )= (CASE when($P{tipReporte} ='T' and $P{codTrim} <> 'T')THEN  $P{codTrim}ELSE '0' END  )
group by    CICLO_ACADEMICO.NOMBRE_CICLO,
VEVAL.ANIO, VEVAL.CICLO,
VEVAL.CODIGO_MATERIA, VEVAL.CODIGO_ENCUESTA,
MATERIA.NOMBRE_PROFESOR,
MATERIA.APELLIDO_PROFESOR, MATERIA.CODIGO_PROFESOR, MATERIA.NOMBRE_MATERIA, MATERIA.TRIMESTRE ,
MATERIA.CODIGO_ESP,
MATERIA.AREA, MATERIA.PROGRAMA,
MATERIA.CODIGO_NIVEL, MATERIA.CODIGO_NIVEACAD]]>
	</queryString>
