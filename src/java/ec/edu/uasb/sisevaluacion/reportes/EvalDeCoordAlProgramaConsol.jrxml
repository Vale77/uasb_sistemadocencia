	<subDataset name="promedioHistorico" uuid="0ed444ff-e300-433e-b678-70cf289c9dc3">
		<parameter name="codArea" class="java.lang.String"/>
		<parameter name="tipReporte" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="codEsp" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[DECLARE @MATERIA TABLE ( CODIGO_ESP VARCHAR(4),  ANIO INT, CODIGO_NIVEACAD VARCHAR(3), CODIGO_FACULTAD NUMERIC(8,0))
INSERT INTO @MATERIA( CODIGO_ESP ,   ANIO , CODIGO_NIVEACAD , CODIGO_FACULTAD )
SELECT distinct   PRG_CODIGO ,  ANIO , COD_NIVEL_ACAD , ARE_CODIGO
FROM interfaseOcu.dbo.PROGRAMA
SELECT DISTINCT VEVAL.ANIO,CONVERT(DECIMAL(8, 2),AVG(VEVAL.VALOR)) AS VALOR
FROM interfaseOcu.EVALUACION.VISTA_TABULACION_EVALDOCENTEANIO AS VEVAL iNNER JOIN @MATERIA AS MATERIA
ON VEVAL.ANIO  = MATERIA.ANIO
AND VEVAL.CODIGO_ESP = MATERIA.CODIGO_ESP
WHERE VEVAL.TIPO_ENCUESTA = 'O'
AND MATERIA.CODIGO_FACULTAD = $P{codArea}
AND (CASE when(($P{tipReporte}='P')and $P{codEsp} <> 'T') THEN VEVAL.CODIGO_ESP ELSE '0' END )= (CASE when(($P{tipReporte} ='P')and $P{codEsp} <> 'T')THEN  $P{codEsp} ELSE '0' END  )
GROUP BY VEVAL.anio]]>
		</queryString>
		<field name="ANIO" class="java.math.BigDecimal"/>
		<field name="VALOR" class="java.math.BigDecimal"/>
	</subDataset>
	<subDataset name="promedioporpregunta" uuid="14a6c831-eb80-4313-b4a9-c4fdf5df5861">
		<parameter name="anio" class="java.lang.String"/>
		<parameter name="codArea" class="java.lang.String"/>
		<parameter name="tipReporte" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="codEsp" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[DECLARE @MATERIA TABLE ( CODIGO_ESP VARCHAR(4), CODIGO_FACULTAD NUMERIC(7,0),  ANIO INT, CODIGO_NIVEACAD VARCHAR(3))
INSERT INTO @MATERIA(CODIGO_ESP , CODIGO_FACULTAD ,  ANIO , CODIGO_NIVEACAD )
SELECT distinct PRG_CODIGO , ARE_CODIGO,ANIO, COD_NIVEL_ACAD
FROM interfaseOcu.dbo.PROGRAMA
where anio = $P{anio}
SELECT DISTINCT  PREG.PREGUNTA_PADRE as CODIGO_PREGUNTA,(SELECT DESCRIPCION FROM interfaseOcu.EVALUACION.PREGUNTA WHERE PREGUNTA.CODIGO_PREGUNTA = PREG.PREGUNTA_PADRE
AND PREGUNTA.CODIGO_ENCUESTA = PREG.CODIGO_ENCUESTA) AS PREGUNTA, CONVERT(DECIMAL(8, 2),AVG( VEVAL.VALOR)) AS VALOR
FROM interfaseOcu.EVALUACION.VISTA_TABULACION_EVALDOCENTEANIO as VEVAL INNER JOIN @MATERIA AS MATERIA
ON VEVAL.ANIO  = MATERIA.ANIO
AND VEVAL.CODIGO_ESP = MATERIA.CODIGO_ESP INNER JOIN interfaseOcu.EVALUACION.PREGUNTA AS PREG WITH (NOLOCK)
ON VEVAL.CODIGO_ENCUESTA = PREG.CODIGO_ENCUESTA
AND VEVAL.CODIGO_PREGUNTA = PREG.CODIGO_PREGUNTA
WHERE VEVAL.tipo_encuesta = 'O'
AND MATERIA.CODIGO_FACULTAD = $P{codArea}
AND VEVAL.ANIO = $P{anio}
AND (CASE when(($P{tipReporte}='P')and $P{codEsp} <> 'T') THEN VEVAL.CODIGO_ESP ELSE '0' END )= (CASE when(($P{tipReporte} ='P')and $P{codEsp} <> 'T')THEN  $P{codEsp} ELSE '0' END  )
GROUP BY PREG.PREGUNTA_PADRE,PREG.CODIGO_ENCUESTA]]>
		</queryString>
		<field name="CODIGO_PREGUNTA" class="java.math.BigDecimal"/>
		<field name="PREGUNTA" class="java.lang.String"/>
		<field name="VALOR" class="java.math.BigDecimal"/>
	</subDataset>
	<parameter name="anio" class="java.lang.String"/>
	<parameter name="tipReporte" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
